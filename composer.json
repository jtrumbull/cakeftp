{
  "name": "jtrumbull/cakeftp",
  "description": "FTP component for CakePHP",
  "keywords": ["CakePHP", "FTP"],
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": ">=3.3.2 <4.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Cakeftp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cakeftp\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
